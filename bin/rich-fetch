#!/usr/bin/env node
const { program } = require('commander');
const shell = require('shelljs');
const chalk = require('chalk');

program
  .name('rich')
  .usage('fetch')
  .on('--help', () => {
    console.log('  Examples:')
    console.log()
    console.log(chalk.gray('    # download objects and refs from another repository'))
    console.log('    $ rich fetch')
    console.log()
  })
  .on('exit',console.log)

function run() {
  shell.exec(`git fetch`)
}
run()